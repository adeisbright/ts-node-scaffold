{
  "name": "ts-node-scaffold",
  "version": "1.0.0",
  "description": "Quickly Scaffold a NodeJS Server App in Typescript",
  "main": "src/index.ts",
  "scripts": {
    "devstart": "ts-node-dev --respawn --pretty --transpile-only src/index.ts",
    "start": "tsc && pm2 start ./dist/index.js --name ts-node-scaffold"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/adeisbright/ts-node-scaffold.git"
  },
  "keywords": [
    "TS",
    "Node",
    "typescript",
    "NodeJS"
  ],
  "author": "Adeleke Bright",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/adeisbright/ts-node-scaffold/issues"
  },
  "homepage": "https://github.com/adeisbright/ts-node-scaffold#readme",
  "dependencies": {
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.0.0",
    "express": "^4.17.3",
    "helmet": "^5.0.2",
    "jsonwebtoken": "^9.0.0",
    "morgan": "^1.10.0",
    "typescript": "^4.6.3",
    "winston": "^3.7.2"
  },
  "devDependencies": {
    "@types/compression": "^1.7.2",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/morgan": "^1.9.3",
    "source-map-support": "^0.5.21",
    "ts-node": "^10.7.0",
    "ts-node-dev": "^1.1.8"
  }
}
